{
  "name": "@pinpoint-fe/utils",
  "version": "0.0.0",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist/**"
  ],
  "scripts": {
    "format": "prettier --write \"./src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "yarn format && eslint ./src --config .eslintrc.cjs",
    "build": "tsup src/index.ts --format esm,cjs --dts --external react",
    "clean": "yarn clean:dist && yarn clean:node_modules && yarn clean:turbo",
    "clean:node_modules": "rm -rf node_modules",
    "clean:dist": "rm -rf dist",
    "clean:turbo": "rm -rf .turbo",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts --external react",
    "devBuild": "tsup src/index.ts --format esm,cjs --dts --external react",
    "test": "jest"
  },
  "dependencies": {
    "@pinpoint-fe/constants": "*",
    "date-fns": "^2.29.3",
    "lodash.clonedeep": "^4.5.0",
    "swr": "^2.1.2"
  },
  "devDependencies": {
    "@pinpoint-fe/configs": "*",
    "@pinpoint-fe/tsconfig": "*",
    "@types/jest": "^28.1.4",
    "@types/lodash.clonedeep": "^4.5.7",
    "@types/react": "^18.0.14",
    "eslint": "^8.15.0",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.2",
    "react": "^18.2.0",
    "ts-jest": "^28.0.5",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  }
}
