{
  "name": "@pinpoint-fe/hooks",
  "version": "0.0.0",
  "license": "Apache-2.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist/**"
  ],
  "scripts": {
    "format": "prettier --write \"./**/*.{js,jsx,ts,tsx}\"",
    "lint": "yarn format && eslint ./src --config .eslintrc.cjs",
    "test": "jest",
    "clean": "yarn clean:dist && yarn clean:node_modules && yarn clean:turbo",
    "clean:node_modules": "rm -rf node_modules",
    "clean:dist": "rm -rf dist",
    "clean:turbo": "rm -rf .turbo",
    "dev": "tsup --config ./tsup.config.dev.ts --watch",
    "devBuild": "tsup --config ./tsup.config.dev.ts",
    "build": "tsup --config ./tsup.config.prod.ts"
  },
  "dependencies": {
    "@pinpoint-fe/constants": "*",
    "@pinpoint-fe/utils": "*",
    "i18next": "^23.2.11",
    "lodash": "^4.17.21",
    "react-i18next": "^13.0.2",
    "usehooks-ts": "^2.14.0"
  },
  "peerDependencies": {
    "@tanstack/react-query": "^5.17.19",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.10.0",
    "swr": "^2.1.2"
  },
  "devDependencies": {
    "@pinpoint-fe/configs": "*",
    "@pinpoint-fe/tsconfig": "*",
    "@types/jest": "^28.1.4",
    "@types/react": "^18.0.14",
    "@types/react-router-dom": "^5.3.3",
    "eslint": "^8.15.0",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.2",
    "react": "^18.2.0",
    "ts-jest": "^28.0.5",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  }
}
